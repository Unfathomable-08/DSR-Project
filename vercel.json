{
    "version": 2, 
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build"
        },
        {
            "src": "users/manage.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/users/(.*)",
            "dest": "user/manage.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*\\.(js|css|svg|png|jpg|jpeg|gif|webp|ico|json|woff|woff2|ttf|otf))",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/src/index.html"
        }
    ]
}