{
    "version": 3,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "src"
        }
      },
      {
        "src": "api/manage.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/manage.py"
      },
      {
        "src": "/(.*)",
        "dest": "/src/index.html"
      }
    ]
  }
  